{
  "name": "unehub",
  "version": "1.0.0",
  "description": "social network for une hooners",
  "main": "server.js",
  "scripts": {
    "start": "docker-compose -f docker-compose.yml up -d",
    "build": "docker-compose -f docker-compose.yml build",
    "build:test": "docker-compose -f docker-compose.test.yml build",
    "server": "NODE_ENV=development nodemon server",
    "format": "prettier --write ./**/*.{js,jsx,json}",
    "test": "docker-compose -f docker-compose.test.yml up --abort-on-container-exit --exit-code-from app"
  },
  "keywords": [
    "nodejs",
    "reactjs",
    "unicycle"
  ],
  "author": "Colby Thomas",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.3.3",
    "crypto": "^1.0.1",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-validator": "^6.1.1",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "libravatar": "^2.0.0",
    "mongoose": "^5.6.11"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "concurrently": "^4.1.2",
    "expect.js": "^0.3.1",
    "husky": "^3.0.5",
    "mem": ">=4.0.0",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "request": "^2.88.0",
    "supertest": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test"
    }
  }
}
